@charset "utf-8";
/* CSS Document */
ul ,li{ margin:0; padding:0;list-style-type:none;}
body{ margin:0; padding:0;font:13px "宋体","Arial Narrow",HELVETICA;background:#fff;-webkit-text-size-adjust:100%;}
*{margin:0;padding:0;list-style-type:none;}
a,img{border:0;}
a:link{color: blue; text-decoration:none;}
a:visited{color:#8B008B;text-decoration: none }
/*a:hover{color:#ea5404;text-decoration:none }
a:active {color: #000000;text-decoration:none }*/

/*表头图片*/
.meddle{ width:1024px; margin:0 auto;}
.header{ width:1024px; float:left;background-repeat:repeat;}
.banner{ background-image:url(../images/logo.jpg); height:200px; margin:0px 0;}

/*搜索框*/
.searchview{width:1024px;height:35px;float:left;background-repeat:repeat;clear: both;margin: 10px 0px 10px 0px;}
/* searchTxt */
.searchBtn button,.searchTxt .searchMenu .searchSelected{background-image:url(../images/searchBtn.png);background-repeat:no-repeat;}

.searchTxt{float:left;width:60%;height:30px;border:2px solid #d3d3d3;border-right:0;position:relative;z-index:20;background:#fff;}
.searchTxt.radius{width:1px;height:1px;overflow:hidden;background:#f4f4f4;position:absolute;top:-2px;left:-2px;}
.searchTxtHover{float:left;width:60%;height:30px;border:2px solid #3297d8;border-right:0;position:relative;z-index:20;background:#fff;}
.searchTxtHover .radius{width:1px;height:1px;overflow:hidden;background:#cce5f5;position:absolute;top:-2px;left:-2px;}

.searchTxt .searchMenu{float:left;}
.searchTxt .searchMenu .searchSelected{color:#a8a8a8;cursor:pointer;font-size:14px;font-weight:bold;height:30px;line-height:30px;padding:0 10px;width:64px;background-position:0px -54px;}
.searchTxt .searchMenu .searchOpen{background-position:0px -104px;}

.searchTxt .searchMenu .searchTab{display:none; position:absolute;top:30px;left:-2px;width:80px;border:2px solid #3297d8;border-top:0;background:#fff;height:115px;z-index:40;}
.searchTxt .searchMenu .searchTab li{width:80px;height:28px;line-height:28px;color:#a8a8a8;font-size:14px;font-weight:bold;text-indent:10px;cursor:pointer;}
.searchTxt .searchMenu .searchTab li.selected{background:#edf3fc;color:#6994c1;}

.searchTxt input{width:392px;float:left;border:0;background:#fff;color:#333;font:14px/22px '宋体',verdana,tahoma,arial,'SimSun',sans-serif;height:22px;margin:0;outline:medium none;padding:4px;}
.searchTxt .sosoLogo{float:right;margin:8px 6px 0 0;display:inline;}
.searchBtn{float:left;}
.searchBtn button{background-position:0px 0px;border:0;color:#fff;cursor:pointer;float:left;font-size:16px;height:34px;text-indent:-9999px;width:86px;}

.questionCL{float:right;background-repeat:repeat;margin:0px auto 0 auto;height: 34px;width: 100px;margin-top: 2px}
.questionCL .questionA{font:25px "宋体", "MICROSOFT YAHEI" !IMPORTANT;}

/*主页内容*/
.content{height: 600px;clear: both;background-color: #ffffff;width:1024px;}
.cntList{height: 200px;width: 320px;background-repeat:repeat;}
.cntLeft{margin-left: 30px}
.cntLeftC{font-family: "MICROSOFT YAHEI" !IMPORTANT;font-weight: bolder;font-size:15px; }
.cntRight{height: 195px;clear: both;width: 320px;margin-left: 10px}
.cntRight1{height: 162px;clear: both;width: 320px;margin-left: 10px}
/*链接*/
.linkb{height: 105px;background-image: url(../images/friend_link.jpg);width: 1024px;background-repeat:repeat;}
.linktitle{height: 30px;width: 1024px;margin-left: 20px;padding-top: 10px}
.gridtext{
	font-size:15px;
}

/*request*/
.searchresult {
width: 1024px;
height: 35px;
float: left;
background-repeat: repeat;
clear: both;
margin: 10px 0px 10px 0px;
background-color:#f1f1f1;
}
.lan{width: 1024px;background-color:#f1f1f1;float:left;background-repeat:repeat;clear: both;padding-left: 25px;padding-right: 25px;}
.sttl { margin: 10px 0; padding: 5px;background: #F5F5F5;}
.mbn { margin-bottom: 5px !important;}
