/*表头图片*/
.meddle{ width:1024px; margin:0 auto;}
.header{ width:1024px; float:left;background-repeat:repeat;height:110px;}
.banner{ background-image:url(../images/ywlogo3.jpg);height:100%;width:100%;margin:0px 0;}

.smallnav {
    height: 38px;
    font-size: 12px;
    color: #6d6d6d;
    line-height: 38px;
}
.divcenter {
    width: 1000px;
    height: auto;
    margin: 0 auto;
    clear: both;
    text-align:center;
}
.newslist {
    width: 998px;
}
.newsleft {
    width: 1000px;
    min-height: 500px;
    float: left;
}
html, body, div, form, ul, li, table, tr, td, span, a, p, img {
    margin: 0px;
    padding: 0px;
    font-family: "宋体";
    font-size: 14px;
}
.headerbk {
    height: 240px;
    clear: both;
    text-align: center;
}
.divall {
    width: 100%;
    height: auto;
    clear: both;
}
h1 {
    display: block;
    font-size: 2em;
    -webkit-margin-before: 0.67em;
    -webkit-margin-after: 0.67em;
    -webkit-margin-start: 0px;
    -webkit-margin-end: 0px;
    font-weight: bold;
}
p {
    display: block;
    -webkit-margin-before: 1em;
    -webkit-margin-after: 1em;
    -webkit-margin-start: 0px;
    -webkit-margin-end: 0px;
}
 .time {
    color: #6d6d6d;
    text-align: center;
    border-bottom: 1px dashed #666666;
    padding-bottom: 15px;
    font-size:14px;
}
.newinfo {
    padding: 20px;
    text-align: left;
    color: #333333;
    line-height: 30px;
}